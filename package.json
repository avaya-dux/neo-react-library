{
  "name": "@avaya/neo-react",
  "version": "1.0.4",
  "description": "This is the React version of the shared library called 'NEO' buit by Avaya",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": "github:avaya-dux/neo-react-library",
  "homepage": "https://design.avayacloud.com/components/web/setup-web#InstallReactviaNodePackageManager",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "source": "src/index.ts",
  "exports": {
    "./avaya-neo-react.css": "./dist/avaya-neo-react.css",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/avaya-neo-react.mjs",
      "require": "./dist/avaya-neo-react.umd.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "LICENSE-3rd-party.html"
  ],
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "scripts": {
    "dev": "vite build --mode dev --watch",
    "storybook16": "storybook dev -p 6006",
    "storybook16+": "NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006",
    "check16": "check-node-version --node 16",
    "storybook": "check-node-version --node 16 && (yarn storybook16 || echo storybook dev failed with node $(node -v)) || (yarn storybook16+ || echo storybook dev failed with node $(node -v))",
    "clean": "rm -rf coverage badges dist storybook-static cypress/screenshots cypress/videos package *.tgz",
    "format": "yarn prettier --write ./src --log-level warn",
    "lint": "eslint src",
    "build": "tsc && vite build",
    "build16:storybook": "storybook build",
    "build16+:storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook build",
    "build:storybook": "check-node-version --node 16 && (yarn build16:storybook || echo build storybook failed) || yarn build16+:storybook || echo build storybook failed with node $(node -v)",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:badges": "yarn test:coverage && jest-coverage-badges --output ./badges",
    "test:cypress": "cypress run --component -b chrome",
    "test:cypress-open": "cypress open --component -b chrome",
    "github": "yarn safe-install && yarn lint && yarn build && yarn test && yarn test:cypress",
    "netlify": "yarn safe-install && yarn build && yarn build:storybook",
    "safe-install": "check-node-version --node 17 && yarn --ignore-engines || yarn",
    "all16+": "yarn safe-install && yarn clean && yarn format && yarn lint && yarn build && yarn build16+:storybook && yarn test:cypress && yarn test:badges && yarn pack",
    "all16": "yarn && yarn clean && yarn format && yarn lint && yarn build && yarn build16:storybook && yarn test:cypress && yarn test:badges && yarn pack",
    "all": "check-node-version --node 16 && (yarn all16 || echo yarn all failed with node $(node -v)) || yarn all16+ || echo yarn all failed with node $(node -v)"
  },
  "dependencies": {
    "atomic-layout": "0.16.2",
    "loglevel": "1.8.1",
    "react-click-away-listener": "2.2.3",
    "react-focus-lock": "2.9.6",
    "react-input-autosize": "3.0.0",
    "styled-components": "6.1.1"
  },
  "peerDependencies": {
    "downshift": "7.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-roving-tabindex": "3.2.0",
    "react-table": "7.8.0"
  },
  "devDependencies": {
    "@avaya/neo": "3.77.0",
    "@avaya/neo-icons": "1.6.0",
    "@storybook/addon-a11y": "7.6.3",
    "@storybook/addon-actions": "7.6.3",
    "@storybook/addon-docs": "7.6.3",
    "@storybook/addon-essentials": "7.6.3",
    "@storybook/addon-interactions": "7.6.3",
    "@storybook/addon-links": "7.6.3",
    "@storybook/blocks": "7.6.3",
    "@storybook/builder-vite": "7.6.3",
    "@storybook/builder-webpack5": "7.6.3",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "7.6.3",
    "@storybook/react-vite": "7.6.3",
    "@storybook/testing-library": "0.2.2",
    "@storybook/testing-react": "2.0.0",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/jest-axe": "3.5.9",
    "@types/node": "20.10.1",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@types/react-input-autosize": "2.2.4",
    "@types/react-table": "7.7.18",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-react": "4.2.0",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "check-node-version": "4.2.1",
    "clsx": "2.0.0",
    "cypress": "13.6.0",
    "cypress-real-events": "1.11.0",
    "diffable-html": "5.0.0",
    "downshift": "7.6.2",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.15",
    "jest-axe": "7.0.1",
    "jest-coverage-badges": "1.1.2",
    "jsdom": "23.0.1",
    "prettier": "3.1.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-roving-tabindex": "3.2.0",
    "react-stopwatch": "2.0.4",
    "react-table": "7.8.0",
    "storybook": "7.6.3",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vite-plugin-dts": "3.6.4",
    "vitest": "0.34.6"
  },
  "resolutions": {
    "unset-value": "2.0.1",
    "minimist": "1.2.8",
    "pug": "3.0.2",
    "uglify-js": "3.17.4",
    "glob-parent": "6.0.2",
    "trim": "1.0.1",
    "trim-newlines": "5.0.0",
    "string-width": "4.2.3"
  }
}
